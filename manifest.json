{
    "manifest_version": 2,
    "name": "Norp Auto Browse",
    "version": "1.0",
    "description": "Automatically loads porn videos 1 after another",
    "icons": {
        "48": "icons/nab-48.png",
        "96": "icons/nab-96.png"
    },
    "background": {
        "scripts": ["./nab.js"],
        "persistent": true,
        "type": "module"
    },
    "permissions": [
        "webRequest",
        "*://*.xnxx.com/*",
        "https://www.xnxx.com/*"
    ],
    "browser_action": {
        "default_icon": {
            "19": "button/nab-19.png",
            "38": "button/nab-38.png"
        },
        "default_title": "Cum Configure Me",
        "default_popup": "popup.html"
    }
}
